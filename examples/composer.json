{
    "name": "symfony/ai-examples",
    "description": "Example scripts about using Symfony AI",
    "type": "project",
    "license": "MIT",
    "require": {
        "ext-pdo": "*",
        "async-aws/bedrock-runtime": "^1.1",
        "codewithkyrian/transformers": "^0.5.3",
        "doctrine/dbal": "^3.3|^4.0",
        "mrmysql/youtube-transcript": "^0.0.5",
        "php-http/discovery": "^1.20",
        "probots-io/pinecone-php": "^1.1",
        "psr/http-factory-implementation": "*",
        "symfony/ai-agent": "@dev",
        "symfony/ai-platform": "@dev",
        "symfony/ai-store": "@dev",
        "symfony/console": "^6.4|^7.0",
        "symfony/css-selector": "^6.4|^7.0",
        "symfony/dom-crawler": "^6.4|^7.0",
        "symfony/dotenv": "^6.4|^7.0",
        "symfony/event-dispatcher": "^6.4|^7.0",
        "symfony/filesystem": "^6.4|^7.0",
        "symfony/finder": "^6.4|^7.0",
        "symfony/process": "^6.4|^7.0",
        "symfony/var-dumper": "^6.4|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Symfony\\AI\\Fixtures\\": "../fixtures/"
        }
    },
    "repositories": [
        { "type": "path", "url": "../src/agent" },
        { "type": "path", "url": "../src/platform" },
        { "type": "path", "url": "../src/store" }
    ],
    "config": {
        "allow-plugins": {
            "codewithkyrian/transformers-libsloader": true,
            "php-http/discovery": true
        },
        "sort-packages": true
    }
}
